<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="PrestashopZhdanova SmokeTest" parallel="classes">
    <listeners>
        <listener class-name="tests.AnnotationTransformer"/>
        <listener class-name="tests.TestListener"></listener>
    </listeners>
    <test name="Thread#1">
        <classes>
            <class name="tests.CreateAccountsTests"/>
            <parameter name="browser" value="fireFox"/>
        </classes>
    </test> <!-- Test -->
    <test name="Thread#2">
        <classes>
            <class name="tests.SingInTests"/>
            <parameter name="browser" value="edge"/>
        </classes>
    </test> <!-- Test -->
</suite>